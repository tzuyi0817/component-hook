{
  "name": "@component-hook/eslint-plugin",
  "type": "module",
  "version": "1.4.2",
  "packageManager": "pnpm@10.29.2",
  "description": "ESLint config for component hook",
  "author": "Tzuyi",
  "license": "MIT",
  "homepage": "https://github.com/tzuyi0817/component-hook/tree/master/packages/eslint-plugin#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tzuyi0817/component-hook.git",
    "directory": "packages/eslint-plugin"
  },
  "bugs": {
    "url": "https://github.com/tzuyi0817/component-hook/issues"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "tag": "latest"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config ../../eslint.config.js",
    "build": "pnpm run build:typegen && tsdown",
    "build:typegen": "unrun scripts/typegen.ts",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "eslint": "catalog:"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@eslint/js": "^10.0.1",
    "@eslint/markdown": "^7.5.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.5.4",
    "eslint-plugin-jsonc": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.7.0",
    "eslint-plugin-yml": "^3.1.2",
    "globals": "^17.3.0",
    "jsonc-eslint-parser": "^2.4.2",
    "typescript-eslint": "^8.55.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@types/eslint-plugin-security": "^3.0.1",
    "eslint-typegen": "^2.3.0",
    "unrun": "^0.2.27"
  }
}
