{
  "name": "@component-hook/eslint-plugin",
  "version": "1.2.0",
  "description": "ESLint config for component hook",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "author": "Tzuyi",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "homepage": "https://github.com/tzuyi0817/component-hook/tree/master/packages/eslint-plugin#readme",
  "bugs": {
    "url": "https://github.com/tzuyi0817/component-hook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tzuyi0817/component-hook.git",
    "directory": "packages/eslint-plugin"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "tag": "latest"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "scripts": {
    "dev": "eslint-config-inspector --config ../../eslint.config.js",
    "build": "tsdown",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "eslint": "^9.15.0"
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.12",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-testing-library": "^7.13.3",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.46.3",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.3.0",
    "@types/eslint-plugin-security": "^3.0.0"
  }
}
