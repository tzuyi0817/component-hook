{
  "name": "vue3-picker",
  "version": "0.4.4",
  "description": "Picker component with vue3",
  "license": "MIT",
  "packageManager": "pnpm@9.3.0",
  "type": "module",
  "author": "Tzuyi",
  "main": "./dist/vue3-picker.umd.js",
  "module": "./dist/vue3-picker.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-picker.es.js",
      "require": "./dist/vue3-picker.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tzuyi0817/vue-componet-library.git",
    "directory": "packages/vue3-picker"
  },
  "homepage": "https://github.com/tzuyi0817/vue-componet-library#readme",
  "bugs": {
    "url": "https://github.com/tzuyi0817/vue-componet-library/issues"
  },
  "publishConfig": {
    "tag": "latest"
  },
  "keywords": [
    "vue",
    "vue3",
    "picker",
    "better-scroll"
  ],
  "scripts": {
    "build": "pnpm typecheck && vite build",
    "typecheck": "bash -c vue-tsc --noEmit",
    "test": "jest",
    "release": "npm publish"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.1",
    "@better-scroll/wheel": "^2.5.1"
  },
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "vite-plugin-dts": "^3.9.1"
  }
