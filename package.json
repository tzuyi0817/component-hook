{
  "license": "MIT",
  "packageManager": "pnpm@9.6.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "dev": "pnpm -C docs dev",
    "build": "pnpm -C docs build",
    "build:picker": "pnpm -C packages/picker build",
    "build:pdf-canvas": "pnpm -C packages/pdf-canvas build",
    "publish:packages": "pnpm publish --recursive --access public --no-git-checks",
    "preview": "pnpm -C docs preview",
    "test:unit": "vitest --coverage",
    "lint": "eslint --fix .",
    "typecheck": "pnpm typecheck:docs && pnpm typecheck:picker && pnpm typecheck:pdf-canvas",
    "typecheck:docs": "pnpm -C docs typecheck",
    "typecheck:picker": "pnpm -C packages/picker typecheck",
    "typecheck:pdf-canvas": "pnpm -C packages/pdf-canvas typecheck",
    "prepare": "husky"
  },
  "peerDependencies": {
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^2.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/vue3-jest": "^29.2.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.8",
    "sass-loader": "^14.2.1",
    "ts-jest": "^29.2.3",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.26"
  },
  "engines": {
    "node": ">= 20.15.1"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "pnpm lint",
      "pnpm typecheck"
    ]
  },
  "optionalDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7"
  }
}
